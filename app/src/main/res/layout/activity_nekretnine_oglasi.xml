<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/toolbar"/>


<android.support.v4.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">






    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        tools:context=".nekretnine_oglasi">



        <Button
            android:id="@+id/stanovi_izdavanje"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="272dp"
            android:text="Stanovi i sobe izdavanje"
            android:background="@drawable/rounded_button"
            android:elevation="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline5"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/guideline"
            app:layout_constraintTop_toTopOf="@+id/guideline3"
            app:layout_constraintVertical_bias="1.0" />




        <Button
            android:id="@+id/stanovi_prodaja"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="192dp"
            android:text="Stanovi prodaja"
            android:background="@drawable/rounded_button"
            android:elevation="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline5"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/guideline"
            app:layout_constraintTop_toBottomOf="@+id/auto_oglasi"
            app:layout_constraintVertical_bias="1.0" />


        <Button
            android:id="@+id/potraznja"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="112dp"
            android:background="@drawable/rounded_button"
            android:elevation="8dp"
            android:text="Potražnja / Kompenzacija"
            app:layout_constraintBottom_toTopOf="@+id/guideline5"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/guideline"
            app:layout_constraintTop_toBottomOf="@+id/posao_oglasi"
            app:layout_constraintVertical_bias="1.0" />

        <Button
            android:id="@+id/odmori_putovanja"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="32dp"
            android:background="@drawable/rounded_button"
            android:elevation="8dp"
            android:text="Odmori i putovanja"
            app:layout_constraintBottom_toTopOf="@+id/guideline5"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/guideline"
            app:layout_constraintTop_toBottomOf="@+id/nekretnine_oglasi"
            app:layout_constraintVertical_bias="1.0" />


        <Button
            android:id="@+id/kuce_prodaja"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:background="@drawable/rounded_button"
            android:elevation="8dp"
            android:text="Kuće prodaja"
            app:layout_constraintBottom_toTopOf="@+id/zdravlje_sport"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/guideline"
            app:layout_constraintTop_toTopOf="@+id/guideline5"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/placevi_prodaja"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="73dp"
            android:background="@drawable/rounded_button"
            android:elevation="8dp"
            android:text="Placevi prodaja"
            app:layout_constraintBottom_toTopOf="@+id/odjeca_obuca"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/guideline"
            app:layout_constraintTop_toTopOf="@+id/guideline5"
            app:layout_constraintVertical_bias="0.0" />


        <Button
            android:id="@+id/poslovni_prostori_izdavanje"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="135dp"
            android:background="@drawable/rounded_button"
            android:elevation="8dp"
            android:text="Poslovni prostori izdavanje"
            app:layout_constraintBottom_toTopOf="@+id/guideline4"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/guideline" />

        <Button
            android:id="@+id/poslovni_prostori_prodaja"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="63dp"
            android:background="@drawable/rounded_button"
            android:elevation="8dp"
            android:text="Poslovni prostori prodaja"
            app:layout_constraintBottom_toTopOf="@+id/guideline4"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/guideline" />

        <Button
            android:id="@+id/vikendici_prodaja"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:background="@drawable/rounded_button"
            android:elevation="8dp"
            android:text="Vikendici prodaja"
            app:layout_constraintBottom_toTopOf="@+id/guideline4"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/guideline" />

        <android.support.constraint.Guideline
            android:id="@+id/guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.05" />

        <android.support.constraint.Guideline
            android:id="@+id/guideline2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.95" />

        <android.support.constraint.Guideline
            android:id="@+id/guideline3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.05" />

        <android.support.constraint.Guideline
            android:id="@+id/guideline4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.95" />

        <android.support.constraint.Guideline
            android:id="@+id/guideline5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.50" />


        <com.google.android.gms.ads.AdView
            android:id="@+id/adView"
            android:layout_width="403dp"
            android:layout_height="53dp"
            android:layout_centerInParent="true"
            app:adSize="BANNER"
            app:adUnitId="ca-app-pub-7799603437316055/4944438446"
            app:layout_constraintBottom_toTopOf="@+id/guideline4"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintStart_toStartOf="@+id/guideline">

        </com.google.android.gms.ads.AdView>


    </android.support.constraint.ConstraintLayout>


</android.support.v4.widget.NestedScrollView>

</LinearLayout>